<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/text_maintanance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints"></TextView>
    <TextView
        android:id="@+id/maintanance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Maintanance:"
        android:textSize="35dp"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:layout_centerHorizontal="true"
        />
    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Name:"
        android:textSize="25dp"
        android:layout_below="@+id/maintanance"
        android:layout_marginTop="20dp"
        android:textColor="@color/black"
        ></TextView>
    <EditText
        android:id="@+id/edname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint=""
        android:textSize="20dp"
        android:layout_below="@+id/maintanance"
        android:layout_marginTop="19dp"
        android:layout_marginStart="120dp"
        ></EditText>
    <TextView
        android:id="@+id/flat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="flat No:"
        android:textSize="25dp"
        android:layout_below="@+id/name"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        ></TextView>
    <EditText
        android:id="@+id/edflat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="--"
        android:textSize="20dp"
        android:layout_below="@+id/edname"
        android:layout_marginTop="12dp"
        android:layout_marginStart="120dp"
        ></EditText>
    <TextView
        android:id="@+id/wing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="wing No:"
        android:textSize="25dp"
        android:layout_below="@+id/flat"
        android:layout_marginTop="30dp"
        android:textColor="@color/black"
        ></TextView>
    <EditText
        android:id="@+id/edwing"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="--"
        android:textSize="20dp"
        android:layout_below="@+id/edflat"
        android:layout_marginTop="12dp"
        android:layout_marginStart="120dp"
        ></EditText>
    <TextView
        android:id="@+id/contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:text="Contact:"
        android:layout_below="@+id/wing"
        android:textColor="@color/black"
        android:layout_marginTop="30dp"></TextView>
    >
    <EditText
        android:id="@+id/edcontact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="-"
        android:textSize="20dp"
        android:layout_below="@+id/edwing"
        android:layout_marginTop="12dp"
        android:layout_marginStart="120dp"

        ></EditText>
    <TextView
        android:id="@+id/ammount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:text="Ammount:"
        android:layout_below="@+id/contact"
        android:textColor="@color/black"
        android:layout_marginTop="30dp"></TextView>
    >
    <EditText
        android:id="@+id/edammount"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="-"
        android:textSize="20dp"
        android:layout_below="@+id/edcontact"
        android:layout_marginTop="12dp"
        android:layout_marginStart="120dp"

        ></EditText>



    <Button
        android:id="@+id/payment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ammount"
        android:text="payment"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="30dp"
        android:layout_centerHorizontal="true"></Button>

<!--    <Button-->
<!--        android:id="@+id/reciept"-->
<!--        android:layout_width="wrap_content"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:layout_below="@+id/ammount"-->
<!--        android:text="Reciept"-->
<!--        android:layout_marginTop="40dp"-->
<!--        android:layout_marginLeft="250dp"></Button>-->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/payment"
        android:textSize="30dp"
        android:text="Thank you!!!!!"
        android:layout_marginTop="40dp"
        android:textColor="@color/black"
        android:layout_marginLeft="100dp"></TextView>

</RelativeLayout>